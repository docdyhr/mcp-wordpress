{
  "include": [
    "src/**/*.ts",
    "dist/**/*.js"
  ],
  "exclude": [
    "src/**/*.d.ts",
    "src/**/*.test.ts",
    "src/types/**",
    "src/docs/**",
    "src/evaluations/**",
    "tests/**",
    "node_modules/**",
    "coverage/**"
  ],
  "reporter": [
    "text",
    "html",
    "json",
    "lcov"
  ],
  "all": true,
  "check-coverage": false,
  "statements": 30,
  "branches": 20,
  "functions": 25,
  "lines": 30
}