{
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "src/**/*.d.ts",
    "src/**/*.test.ts",
    "src/types/**",
    "tests/**"
  ],
  "reporter": [
    "text",
    "html",
    "json"
  ],
  "all": true,
  "check-coverage": false,
  "statements": 30,
  "branches": 20,
  "functions": 25,
  "lines": 30
}